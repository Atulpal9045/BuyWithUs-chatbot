        "express": "^4.17.1",
        "express-browserify": "^1.0.3",
        "express-rate-limit": "^5.2.6",
        "http": "0.0.1-security",
        "ibm-watson": "^5.7.3",
        "inline-worker": "^1.1.0",
        "jquery": "^3.6.0",
        "mongoose": "^5.12.3",
